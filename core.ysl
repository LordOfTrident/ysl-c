func equals n1 n2
	param n1
	param n2

	addr n2
	asm mov si, ax
	asm mov bx, [si]
	addr n1
	asm mov si, ax
	asm mov ax, [si]
	asm cmp ax, bx
	asm je .equals
	asm mov ax, 0
	asm ret
	asm .equals:
	asm mov ax, 1
	asm ret
endf

func less n1 n2
	param n1
	param n2

	addr n2
	asm mov si, ax
	asm mov bx, [si]
	addr n1
	asm mov si, ax
	asm mov ax, [si]
	asm cmp ax, bx
	asm jl .less
	asm mov ax, 0
	asm ret
	asm .less:
	asm mov ax, 1
	asm ret
endf

func greater n1 n2
	param n1
	param n2

	addr n2
	asm mov si, ax
	asm mov bx, [si]
	addr n1
	asm mov si, ax
	asm mov ax, [si]
	asm cmp ax, bx
	asm jg .greater
	asm mov ax, 0
	asm ret
	asm .greater:
	asm mov ax, 1
	asm ret
endf

func peek8 address
	param address

	addr address
	asm mov si, ax
	asm mov di, [si]
	asm mov al, [di]
	asm ret
endf

func poke8 address value
	param address
	param value

	addr address
	asm mov si, ax
	asm mov di, [si]
	addr value
	asm mov si, ax
	asm mov al, [si]
	asm mov [di], al
	asm ret
endf

func peek16 address
	param address

